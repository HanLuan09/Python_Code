t=int(input())
for i in range(t):
    s=input()
    kq=""
    i=1
    dem=1
    while i<len(s):
        if(s[i]==s[i-1]):
            dem=dem+1
        else : 
            print(dem,end="")
            print(s[i-1],end="")
            dem=1
        i+=1
    print(dem,end="")
    print(s[len(s)-1],end="")
    print()
