t=int(input())
for i in range(t):
    n=int(input())
    b={}
    a=input().split()
    for i in a:
        if i in b:
            b[i]+=1
        else : b[i]=1
    s=0
    p=0
    for i in a:
        if s<b[i]:
            s=b[i]
            p=i
    if s<= n/2:
        print("NO")
    else : print(p)
